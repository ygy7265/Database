 */
public class ForTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//for 
		for(int i = 0; i <= 5 ; i++) {
		//	System.out.println("i = " + i);
		}
		//1부터 10까지 합
		int sum = 0;
		for(int k = 0; k>=10 ; k++) {
			System.out.println(sum + k);
		}
		//1부터 10까지 짝수합
		//for 중첩
		//구구단
		//별삼각형
	}

}
