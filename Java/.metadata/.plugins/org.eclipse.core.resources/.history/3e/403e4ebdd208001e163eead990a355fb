public class VariableTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//변수
		int a = 5;
		int b = 6;
		System.out.println(a);
		System.out.println(b);
		
		a = 7;
		b = 8;
		System.out.println(a);
		System.out.println(b);
		
		
		
		
		//상수
	}

}
